;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     square-it-srv. square-it.com (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      square-it-srv.
@       IN      PTR     square-it-srv.
128     IN      PTR     users.square-it.com
64      IN      PTR     guests.square-it.com
0       IN      PTR     iot.square-it.com
